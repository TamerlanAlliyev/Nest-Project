@model FooterSub
<div class="container mt-5">
    <div class="card">
        <div class="card-body mt-5 ml-5 ">
            <div class="row">

                <div class="col-lg-10 col-md-2 col-sm-10 ml-3 mr-3">

                    <div class="d-flex justify-content-center align-items-center">

                        <div class="mr-5">
                            <h3 class="box-title mb-4">@Model.SubList</h3>
                            <td class="text-center">
                                <a asp-area="Admin" asp-controller="FooterNavbar" asp-action="SubListUpdate" asp-route-id="@Model.Id" asp-route-head="@Model.FooterHeads.Id" class="btn btn-sm btn-warning">Edit</a>
                            </td>
                            <td class="text-center">
                                <a asp-area="Admin" asp-controller="FooterNavbar" asp-action="SubListDelete" asp-route-id="@Model.Id" asp-route-head="@Model.FooterHeads.Id" class="btn btn-sm btn-danger">Remove</a>
                            </td>
                        </div>
                    </div>

                </div>

                <div class="col-lg-12 col-md-12 col-sm-12">
                    <h3 class="box-title mt-5">Sub List</h3>
                    <div class="table-responsive">
                        <table class="table table-striped table-product">
                            <tbody>

                                <tr>
                                    <td><h5>Head : </h5> </td> 
                                    <td>@Model.FooterHeads.Head</td></tr>

                                <tr>
                                    <td><h5>Order : </h5> </td>
                                    <td>@Model.Order</td></tr>

                                <tr>
                                    <td><h5>Created : </h5> </td>
                                    <td>@Model.Created</td></tr>

                                <tr>
                                    <td><h5>CreateBy : </h5> </td>
                                    <td>@Model.CreateBy</td></tr>

                                <tr>
                                    <td><h5>Modified : </h5> </td>
                                    <td>@Model.Modified</td></tr>

                                <tr>
                                    <td><h5>ModifiedBy :  </h5> </td>
                                    <td>@Model.ModifiedBy</td></tr>

                                <tr>
                                    <td><h5>IPAddress :  </h5> </td>
                                    <td>@Model.IPAddress</td></tr>






                            </tbody>
                        </table>
                    </div>


                </div>



            </div>
        </div>
    </div>
    <div class="d-flex justify-content-center align-items-center flex-column text-center mt-5">
        <h5>Come Back for @Model.FooterHeads.Head</h5>
        <a class="btn btn-primary mt-3 mb-5 w-25" asp-area="Admin" asp-controller="FooterNavbar" asp-action="Detail" asp-route-id="@Model.FooterHeads.Id">Come Back</a>
    </div>

</div>